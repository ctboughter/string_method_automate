set step              0

structure             ../vac_protein2.psf

coordinates           ../vac_protein2.pdb
#set inputname      input.0;
#binCoordinates     $inputname.coor;
#binVelocities      $inputname.vel;
#extendedSystem     $inputname.xsc;
outputname            input.$step
outputEnergies        1000
outputTiming          1000
dcdfreq               500
dcdUnitCell           yes

paraTypeCharmm        on
parameters            ../toppar/par_all36m_prot.prm
#### Need to load in these other things so water_ions doesnt crash
parameters            ../toppar/par_all36_na.prm
parameters            ../toppar/par_all36_carb.prm
parameters            ../toppar/par_all36_lipid.prm
parameters            ../toppar/par_all36_cgenff.prm
parameters            ../toppar/toppar_water_ions.str
#### And add this shit just cause
parameters            ../toppar/toppar_dum_noble_gases.str
parameters            ../toppar/toppar_all36_prot_d_aminoacids.str
parameters            ../toppar/toppar_all36_prot_fluoro_alkanes.str
parameters            ../toppar/toppar_all36_prot_heme.str
parameters            ../toppar/toppar_all36_prot_na_combined.str
parameters            ../toppar/toppar_all36_prot_retinol.str
parameters            ../toppar/toppar_all36_na_nad_ppi.str
parameters            ../toppar/toppar_all36_na_rna_modified.str


# Force-Field Parameters
exclude               scaled1-4
1-4scaling            1.0
cutoff                12.0
switching             on
switchdist            10.0
pairlistdist          14.5
vdwForceSwitching     yes

# Integrator Parameters
timestep              2.0  
rigidBonds            all  
nonbondedFreq         1
fullElectFrequency    1 
stepspercycle         20

# Constant Temperature Control
temperature           300.15 # Only here when there is no initial coors
langevin              on    
langevinDamping       1     
langevinTemp          300.15
langevinHydrogen      off 

dielectric 80

colvars               on
source ../CVs.tcl
colvarsConfig         ../colvars.conf 
colvarbias changeconfig alpha "centers {3.18492e+02}   forceconstant 0.1"
colvarbias changeconfig beta  "centers {1.27323e+02}   forceconstant 0.1"
colvarbias changeconfig gamma "centers {3.78397e+01}   forceconstant 0.1"
colvarbias changeconfig theta "centers {8.98307e+01}   forceconstant 0.1"
colvarbias changeconfig phi   "centers {5.23606e+01}   forceconstant 0.1"
colvarbias changeconfig dist  "centers {3.84537e+01}   forceconstant 10.0"
colvarbias changeconfig Pin   "centers {2.04416e+01}   forceconstant 1.0"

minimize 1000
run 500000
